{
  "name": "vue-vite-components",
  "version": "0.0.0-development-7",
  "files": [
    "dist"
  ],
  "main": "./dist/vue-vite-components.umd.js",
  "module": "./dist/vue-vite-components.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue-vite-components.es.js",
      "require": "./dist/vue-vite-components.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "test:unit": "vitest --environment jsdom",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress run'",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "chromatic": "npx chromatic --project-token=99965cdc00f9",
    "coverage": "vitest run --coverage --environment jsdom",
    "publish:package": "npm run build && npm publish"
  },
  "dependencies": {
    "@heroicons/vue": "^1.0.6",
    "@storybook/addon-docs": "^6.4.21",
    "@storybook/builder-webpack5": "^6.4.21",
    "@storybook/manager-webpack5": "^6.4.21",
    "i": "^0.3.7",
    "npm": "^8.6.0",
    "pinia": "^2.0.13",
    "sass-loader": "^12.6.0",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@rushstack/eslint-patch": "^1.1.0",
    "@storybook/addon-a11y": "^6.4.20",
    "@storybook/addon-actions": "^6.4.20",
    "@storybook/addon-essentials": "^6.4.20",
    "@storybook/addon-links": "^6.4.20",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/vue3": "^6.4.20",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^16.11.26",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.9",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.18",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.4",
    "babel-loader": "^8.2.4",
    "c8": "^7.11.0",
    "chromatic": "^6.5.3",
    "cypress": "^9.5.3",
    "eslint": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^8.2.0",
    "jsdom": "^19.0.0",
    "postcss": "^8.4.12",
    "prettier": "^2.5.1",
    "sass": "^1.49.10",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.0.23",
    "typescript": "~4.6.3",
    "vite": "^2.9.1",
    "vitest": "^0.8.1",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^0.33.9"
  },
  "resolutions": {
    "webpack": "^5"
  }
}
